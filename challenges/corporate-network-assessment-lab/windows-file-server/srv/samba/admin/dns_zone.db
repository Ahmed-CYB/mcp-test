$TTL 86400
@   IN  SOA corpnet.local. admin.corpnet.local. (
        2024011501  ; Serial
        3600        ; Refresh
        1800        ; Retry
        604800      ; Expire
        86400       ; Minimum TTL
)

@               IN  NS      dc1.corpnet.local.
@               IN  A       192.168.1.10
dc1             IN  A       192.168.1.10
file-server     IN  A       192.168.1.20
web-server      IN  A       192.168.1.30
db-server       IN  A       192.168.1.40
backup-server   IN  A       192.168.1.50
print-server    IN  A       192.168.1.60

; Service records
_ldap._tcp      IN  SRV     0 5 389 dc1.corpnet.local.
_kerberos._tcp  IN  SRV     0 5 88  dc1.corpnet.local.
_gc._tcp        IN  SRV     0 5 3268 dc1.corpnet.local.

; Administrative records
admin           IN  CNAME   dc1.corpnet.local.
backup          IN  CNAME   backup-server.corpnet.local.
ftp             IN  CNAME   file-server.corpnet.local.

; Internal service discovery
_snmp._udp      IN  SRV     0 5 161 file-server.corpnet.local.
_winrm._tcp     IN  SRV     0 5 5985 file-server.corpnet.local.
_rdp._tcp       IN  SRV     0 5 3389 file-server.corpnet.local.

; Development/Test entries (SHOULD BE REMOVED)
test-admin      IN  A       192.168.1.100
dev-backup      IN  A       192.168.1.101
secret-share    IN  CNAME   file-server.corpnet.local.