      <Basic>true</Basic>
      <Digest>true</Digest>
      <Kerberos>true</Kerberos>
      <Negotiate>true</Negotiate>
      <Certificate>true</Certificate>
      <CredSSP>false</CredSSP>
    </Auth>
    <DefaultPorts>
      <HTTP>5985</HTTP>
      <HTTPS>5986</HTTPS>
    </DefaultPorts>
    <TrustedHosts>*</TrustedHosts>
  </Client>
</Configuration>