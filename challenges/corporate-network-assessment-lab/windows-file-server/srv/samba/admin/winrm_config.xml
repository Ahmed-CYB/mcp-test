    <MaxConnections>300</MaxConnections>
    <MaxPacketRetrievalTimeSeconds>120</MaxPacketRetrievalTimeSeconds>
    <AllowUnencrypted>true</AllowUnencrypted>
    <Auth>
      <Basic>true</Basic>
      <Kerberos>true</Kerberos>
      <Negotiate>true</Negotiate>
      <Certificate>false</Certificate>
      <CredSSP>false</CredSSP>
    </Auth>
    <DefaultPorts>
      <HTTP>5985</HTTP>
      <HTTPS>5986</HTTPS>
    </DefaultPorts>
