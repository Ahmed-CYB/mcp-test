;
; BIND data file for lab.local
;
$TTL    604800
@       IN      SOA     ubuntu-webserver.lab.local. admin.lab.local. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      ubuntu-webserver.lab.local.
@       IN      A       192.168.1.100
ubuntu-webserver IN A   192.168.1.100
www     IN      A       192.168.1.100
ftp     IN      A       192.168.1.100
mail    IN      A       192.168.1.100
tftp    IN      A       192.168.1.100
scanner-kali IN A       192.168.1.50
centos-db IN   A       192.168.1.101
debian-file IN A       192.168.1.102
arch-dev IN    A       192.168.1.103